@{
    ViewBag.Title = "Видео контент некоторых брендов представленых на нашем сайте";
    Layout = Request.Params["part"] != null && Request.Params["part"] == "true" ? null : "~/Views/Shared/_Layout.cshtml";
}
@section script_section{
    <script type="text/javascript">

        $(function () {
            $(".btn").button("loading");
        });
    </script>
    }

 <h2 class="featurette-heading">@ViewBag.Title</h2>
<div id="video_manders">
    <hr class="featurette-divider">
    <h2 class="featurette-heading">Little Greene <span class="muted">видео</span></h2>
    <a class="btn" data-toggle="collapse" data-parent="#video_manders" href="#video_Little_Greene">
        
    </a>
    <div id="video_Little_Greene" class="accordion-body collapse in">@Html.Partial("Products/part_LG/video_Little_Greene")</div>
    
    <hr class="featurette-divider">
    <h2 class="featurette-heading">Farrow & Ball <span class="muted">видео</span></h2>

    <a class="btn" data-toggle="collapse" data-parent="#video_manders" href="#video_Farrow_and_Ball">
    </a>
    <div id="video_Farrow_and_Ball" class="accordion-body collapse">@Html.Partial("Products/part_FB/video_Farrow_and_Ball")</div>
    
    <hr class="featurette-divider">
    <h2 class="featurette-heading">Sanderson <span class="muted">видео</span></h2>
    <button data-parent="#video_manders" type="button" class="btn btn-danger" data-toggle="collapse" data-target="#video_Sanderson" data-loading-text="Загрузка видео контента..."autocomplete="off">
        Загрузить видео контент...

    </button>
    <div id="video_Sanderson" class="collapse">@Html.Partial("Products/part_Sn/video_Sanderson")</div>
    
    <hr class="featurette-divider">
    <h2 class="featurette-heading">William Howard <span class="muted">видео</span></h2>
    <button data-parent="#video_manders" type="button" class="btn btn-info" data-toggle="collapse" data-target="#video_William_Howard" data-loading-text="Загрузка видео контента..." autocomplete="off">
        Загрузить видео контент...
    </button>
    <button type="button" class="btn btn-primary" data-loading-text="Loading..." autocomplete="off">Loading state</button>
    <div id="video_William_Howard" class="collapse">@Html.Partial("Products/part_WH/video_William_Howard")</div>
    <hr class="featurette-divider">
</div>
   



