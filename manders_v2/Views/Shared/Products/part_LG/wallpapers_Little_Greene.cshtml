    <h2 class="featurette-heading">ОБОИ XIX ВЕКА</h2>
    
<div class="featurette">
            <table class="table table-condensed">
                <tr>
                    <td rowspan="2" style="border: 0 !important; text-align: right;">
                        <img class="ui-corner-all" width="400" src="@Url.Content("~/Content/images/LG/wallpaper/little-greene-great-ormond-street-parchment.jpg")" alt="ОБОИ XIX ВЕКА"/>
                    </td>
                    <td style="border: 0 !important; text-align: left;">
                        <img class="ui-corner-all" width="200" src="@Url.Content("~/Content/images/LG/wallpaper/XIX.jpg")" /></td>
                    
                    
                </tr>
